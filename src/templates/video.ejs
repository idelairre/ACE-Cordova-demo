<header>
    <div id="close-btn">
        <a id="close-btn" href="#">x</a>
    </div>
</header>
<div id="cbp-fwslider" class="cbp-fwslider">
    <ul>
        <!-- Each child div in #showcase represents a slide -->
        <% videos.forEach(function (video) { %>
        <li>
            <video controls>
                <source src="<%= require('../assets/video/' + video.id) %>">
            </video>
        </li>
        <% }) %>
    </ul>
</div>
